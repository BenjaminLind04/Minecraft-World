for i=1,30,1 do
   turtle.back()
   turtle.select(1)
   for i=1,5,1 do
     turtle.place()
     turtle.down()
     turtle.back()
   end
   turtle.forward()
   turtle.down()
   turtle.digDown()
   turtle.down()
   turtle.digDown()
   turtle.select(3)
   turtle.placeDown()
   turtle.up()
   turtle.select(2)
   turtle.placeDown()
   turtle.up()
   turtle.select(4)
   turtle.placeDown()
   turtle.back()
   turtle.down()
   turtle.down()
end
