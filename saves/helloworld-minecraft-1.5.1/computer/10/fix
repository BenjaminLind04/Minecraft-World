for i=1,20,1 do
  turtle.select(1)
  for i=1,2,1 do
    turtle.digDown()
    turtle.down()
  end
  turtle.digDown()
  turtle.placeDown()
  for j=1,2,1 do
    turtle.up()
    turtle.select(j+1)
    turtle.placeDown()
  end
  turtle.back()
  turtle.down()
  turtle.select(4)
  turtle.placeDown()
  for k=1,5,1 do
    turtle.back()
    turtle.down()
  end 
end
